{
    "additional-repositories": [],
    "archinstall-language": "English",
    "audio_config": null,
    "bootloader": "Systemd-boot",
    "config_version": "3.0.2",
    "disk_config": null,
    "disk_encryption": null,
    "hostname": "minux",
    "kernels": [
        "linux-lts"
    ],
    "locale_config": {
        "kb_layout": "us",
        "sys_enc": "UTF-8",
        "sys_lang": "en_US"
    },
    "mirror_config": {
        "custom_mirrors": [],
        "mirror_regions": {
            "South Korea": [
                "http://ftp.kaist.ac.kr/ArchLinux/$repo/os/$arch",
                "http://kr.mirrors.cicku.me/archlinux/$repo/os/$arch",
                "https://kr.mirrors.cicku.me/archlinux/$repo/os/$arch",
                "http://ftp.lanet.kr/pub/archlinux/$repo/os/$arch",
                "https://ftp.lanet.kr/pub/archlinux/$repo/os/$arch",
                "http://mirror.siwoo.org/archlinux/$repo/os/$arch",
                "https://mirror.siwoo.org/archlinux/$repo/os/$arch",
                "http://mirror.funami.tech/arch/$repo/os/$arch",
                "https://mirror.funami.tech/arch/$repo/os/$arch",
                "http://mirror.yuki.net.uk/archlinux/$repo/os/$arch",
                "https://mirror.yuki.net.uk/archlinux/$repo/os/$arch"
            ]
        }
    },
    "network_config": {
        "type": "iso"
    },
    "ntp": true,
    "packages": [
        "git",
        "nano"
    ],
    "parallel downloads": 5,
    "profile_config": null,
    "save_config": null,
    "swap": true,
    "timezone": "Asia/Seoul",
    "uki": false,
    "version": "3.0.2",
    "custom-commands": [
        "git clone https://github.com/kuper0201/minux_scripts /root/minux_scripts",
        "cd /root/minux_scripts && ./setup.sh",
        "rm -rf /root/minux_scripts"
    ]
}
